    while(!place_meeting(x + sign(hsp), y, wallObject)) {
        x += sign(hsp)
    }
} else {
    x += hsp
}

if place_meeting(x, y + vsp, wallObject) {
    while(!place_meeting(x, y + sign(vsp), wallObject)) {
        y += sign(vsp)
    }
} else {
    y += vsp
}

</string>
