keyDown = keyboard_check(ord("S"))

hsp = playerSpeed * ((-1 * keyLeft) + keyRight)
vsp = playerSpeed * ((-1 * keyUp) + keyDown)

if hsp != 0 &amp;&amp; vsp != 0 {
    hsp = round(hsp/sqrt(2))
    vsp = round(vsp/sqrt(2))
}

